@Test
public void testFeedRecordsBeansListPreparedStatement() throws Exception {
    // Setup test data
    String branch = "BRANCH";
    Timestamp beginDate = new Timestamp(System.currentTimeMillis());
    Timestamp endDate = new Timestamp(System.currentTimeMillis());

    // Set the private fields via getters/setters or reflection
    P06_IACC_01_Main main = new P06_IACC_01_Main();
    main.setG_s_branch(branch); // assuming you have a public setter
    main.setG_o_begindate(beginDate);
    main.setG_o_endDate(endDate);

    // Mock PreparedStatement
    PreparedStatement mockPreparedStatement = mock(PreparedStatement.class);

    // Use reflection to call the private method
    Method method = P06_IACC_01_Level1DBTransaction.class.getDeclaredMethod(
        "feedRecordsBeansListPreparedStatement", PreparedStatement.class);
    method.setAccessible(true);
    
    // Instance of the class that contains the method
    P06_IACC_01_Level1DBTransaction dbTransaction = new P06_IACC_01_Level1DBTransaction(main);
    
    // Invoke the method with the mock PreparedStatement
    method.invoke(dbTransaction, mockPreparedStatement);

    // Verify the correct call on PreparedStatement
    verify(mockPreparedStatement).setTimestamp(3, Outlits.getSqlTimestamp(main.getG_o_endDate()));
}
