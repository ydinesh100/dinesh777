private static void showUsage() {
    System.out.println("""
        Usage: <level> <branch> <event_date> [<begin date> <end date>] [file name]
            where:
                level: 2 possible values Level1/Level2
                branch: branch name
                event_date: processing date, DD/MM/YYYY hh:mi:ss formatted
                begin_date: begin date, DD/MM/YYYY hh:mi:ss formatted (for level 1)
                end_date: end date, DD/MM/YYYY hh:mi:ss formatted (for level 1)
                file name: report file name (for level 2)
        """);
}
