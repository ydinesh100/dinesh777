import org.junit.Before;
import org.junit.Test;
import static org.junit.Assert.*;

public class CollateralServiceImplTest {

    private CollateralServiceImpl service;

    @Before
    public void setUp() {
        service = new CollateralServiceImpl();
    }

    @Test
    public void testGetPopulationAndMainDataRequest_ReturnsConstant() {
        String expected = CollateralServiceImpl.COLLATERAL_POPULATION_AND_MAIN_DATA_REQUEST;
        String actual = service.getPopulationAndMainDataRequest();
        assertEquals(expected, actual);
    }

    @Test
    public void testGetBasicPopulationRequest_ReturnsConstant() {
        String expected = CollateralServiceImpl.COLLATERAL_BASIC_POPULATION_REQUEST;
        String actual = service.getBasicPopulationRequest();
        assertEquals(expected, actual);
    }
}
