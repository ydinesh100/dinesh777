@Test
public void testExecute_withException_shouldEnterCatchBlock() throws Exception {
    // Arrange
    P06_IREP_01_Level<?> levelUnderTest = new P06_IREP_01_LevelTestSubclass();

    MockedStatic<GlobalAtlentic> globalMock = Mockito.mockStatic(GlobalAtlentic.class);
    globalMock.when(GlobalAtlentic::getLoanIQConnection)
              .thenThrow(new SQLException("Simulated connection error"));

    try {
        levelUnderTest.execute(); // âœ… method now recognized
    } catch (P06_IREP_01_ExitApplicationException e) {
        // Expected exception
    }

    globalMock.close();
}
